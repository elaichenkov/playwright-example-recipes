{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2017",
    "lib": [
      "es2018",
      "DOM",
    ],
    "out": "lib",
    "sourceMap": false,
    "exclude": ["node_modules"]
  }
}